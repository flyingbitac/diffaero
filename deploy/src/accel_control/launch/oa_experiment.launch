<launch>
    <include file="$(find accel_control)/launch/px4_optitrack.launch">
        <arg name="vrpn_server_ip"     value="192.168.1.3"/>
    </include>

    <include file="$(find accel_control)/launch/realsense_camera.launch"/>

    <include file="$(find accel_control)/launch/oa_node.launch">
        <arg name="path"               value="/home/orin/ws/checkpoints/oa_bptt.pt2"/>
        <!-- <arg name="path"               value="/home/orin/ws/checkpoints/oa_dreamer.pt2"/> -->
        <arg name="use_cuda"           value="true"/>

        <arg name="max_acc"            value="10."/>
        <arg name="max_vel"            value="5."/>
        <arg name="hover_thrust"       value="0.707"/>
        <arg name="control_freq"       value="30"/>

        <arg name="home_x"             value="0"/>
        <arg name="home_y"             value="0"/>
        <arg name="target_x"           value="54"/>
        <arg name="target_y"           value="54"/>
        <arg name="height"             value="3"/>

        <arg name="max_dist"           value="10."/>
        <arg name="image_height"       value="9"/>
        <arg name="image_width"        value="16"/>
        <arg name="display"            value="true"/>
    </include>
</launch>