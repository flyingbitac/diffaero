<launch>
	<include file="$(find accel_control)/launch/px4_optitrack.launch">
        <arg name="vrpn_server_ip"     value="192.168.1.3"/>
	</include>

	<include file="$(find accel_control)/launch/pc_node.launch">
        <arg name="path"               value="/home/orin/ws/checkpoints/pc_bptt.pt2"/>
        <arg name="use_cuda"           value="true"/>

        <arg name="max_acc"            value="10."/>
        <arg name="max_vel"            value="2."/>
        <arg name="hover_thrust"       value="0.707"/>
        <arg name="control_freq"       value="30"/>
        
        <arg name="home_x"             value="-2"/>
        <arg name="home_y"             value="0"/>
        <arg name="target_x"           value="2.6"/>
        <arg name="target_y"           value="0.5"/>
        <arg name="height"             value="1.5"/>
	</include>
</launch>